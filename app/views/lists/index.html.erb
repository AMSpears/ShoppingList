   <% if current_user %>
<div class= "container">
<section>
  <div class="create_button">
  <%= link_to "Create a New List ", new_list_path , :class => "new_list"%>
  </div>
  <% current_user.lists.each do |list| %>
  <ul>
    <li>
    <%= link_to list.title, list_path(list) %> <time>Updated <%= list.updated_at %></time>
  </li>
</ul>
  <% end %>
  <% end %>
</section>
</div>
